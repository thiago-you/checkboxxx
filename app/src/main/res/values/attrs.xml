<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CheckBoxxx">
        <attr name="indeterminate_state" format="boolean" />
        <attr name="unchecked_state" format="boolean" />

        <attr name="state" format="enum">
            <enum name="indeterminate" value="-1"/>
            <enum name="unchecked" value="0"/>
            <enum name="checked" value="1"/>
        </attr>
    </declare-styleable>
</resources>